@model Library.Models.User


@{
    ViewBag.Title = "User";
}


<h2>Profile Information</h2>


<div>
    <h4>User</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Username:
        </dt>
        <dd>
            @Model.user_name
        </dd>
        <dt>
            Age:
        </dt>
        <dd>
            @Model.user_age
        </dd>
        <dt>
            Address:
        </dt>
        <dd>
            @Model.user_address
        </dd>
        <dt>
            Zipcode:
        </dt>
        <dd>
            @Model.user_zipcode
        </dd>
        <dt>
            Mail:
        </dt>
        <dd>
            @Model.user_mail
        </dd>
    </dl>
           
</div>
<div>
        @Html.ActionLink("Back to Homepage", "Index") 
</div>
